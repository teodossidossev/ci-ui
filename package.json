{
  "name": "ci-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "eslint 'src/**'",
    "lint:fix": "eslint --fix 'src/**'",
    "start:ci": "http-server ./dist/ci-ui -a localhost -p 4200 -c-1",
